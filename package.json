{
  "name": "activity_buddies",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-css": "tailwindcss build -i ./src/input.css -o ./public/output.css",
    "watch-css": "tailwindcss build -i ./src/input.css -o ./public/output.css --watch",
    "devi": "concurrently \"npm run watch-css\" \"npm dev\"",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/css": "^11.10.8",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@google-cloud/storage": "^6.10.1",
    "dotenv": "^16.0.3",
    "firebase": "^9.21.0",
    "flowbite": "^1.6.5",
    "flowbite-react": "^0.4.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-google-button": "^0.7.2",
    "react-icons": "^4.8.0",
    "react-router": "^6.11.0",
    "react-router-dom": "^6.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
